<div class="story">
  <div class="row">
    <div class="story-header col-md-2">
      <img class="content-thumbnail" src="assets/images/bigbird.png">
      {{name}}
    </div>
    <div class="story-body col-md-10">
      {{name}}: {{city}}, {{state}}
      <br>
      <button type="button" class="btn btn-default visit-single-resource visit-gym" data-link-id="{{id}}">
        Visit
      </button>
    </div>
  </div>
</div>
