<div class="edit-climb-square-preview edit-climb bk-{{color}}" data-edit-climb-id="{{id}}" data-edit-climb-color="{{color}}" data-edit-climb-type="{{climb_type}}" data-edit-climb-grade="{{grade}}" data-edit-climb-modifier="{{modifier}}">
  {{climb_type}}{{grade}}{{modifier}}
</div>
<fieldset class="edit-climb">
  <input type="hidden" class="form-control editInputClimbColor" data-edit-climb-id="{{id}}" name="color" value="{{color}}">
  <input type="hidden" class="form-control editInputClimbClimb_Type" data-edit-climb-id="{{id}}" name="climb_type" value="{{climb_type}}">
  <input type="hidden" class="form-control editInputClimbGrade" data-edit-climb-id="{{id}}" name="grade" value="{{grade}}">
  <input type="hidden" class="form-control editInputClimbModifier" data-edit-climb-id="{{id}}" name="modifier" value="{{modifier}}">
  <input type="hidden" class="form-control editInputClimbGym" data-edit-climb-id="{{id}}" name="gym_id" value="{{gym_id}}">
</fieldset>
